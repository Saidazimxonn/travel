    <script>
        function latKirl(latvalue) {
            let value1 = [];
            let latlegh = latvalue.split("");
            for (let i = 0; i < latlegh.length; i++) {
                value1[i] = latvalue;
                latvalue = value1[i].replace("Ya", "Я").replace("ya", "я").replace("Yu", "Ю").replace("yu", "ю").replace("Ch", "Ч").replace("ch", "ч")
                    .replace("Sh", "Ш").replace("sh", "ш").replace("Ts", "Ц").replace("ts", "ц").replace("O‘", "Ў").replace("o‘", "ў").replace("Yo", "Ё")
                    .replace("yo", "ё").replace("Ye", "Е").replace("ye", "е").replace("E", "Э").replace("G‘", "Ғ").replace("g‘", "ғ");
            }
            for (let i = 0; i < latvalue.length; i++) {
                value1[i] = latvalue;
                latvalue = value1[i].replace("A", "А").replace("a", "а").replace("B", "Б").replace("b", "б").replace("V", "В").replace("v", "в").replace("e", "е")
                    .replace("G", "Г").replace("g", "г").replace("D", "Д").replace("d", "д").replace("Z", "З").replace("z", "з").replace("L", "Л").replace("l", "л")
                    .replace("I", "И").replace("i", "и").replace("Y", "Й").replace("y", "й").replace("K", "К").replace("k", "к").replace("p", "п").replace("R", "Р")
                    .replace("M", "М").replace("m", "м").replace("N", "Н").replace("n", "н").replace("O", "О").replace("o", "о").replace("P", "П").replace("'", "ъ")
                    .replace("r", "р").replace("Q", "Қ").replace("q", "қ").replace("S", "С").replace("s", "с").replace("T", "Т").replace("t", "т").replace("U", "У")
                    .replace("u", "у").replace("F", "Ф").replace("f", "ф").replace("H", "Ҳ").replace("h", "ҳ").replace("X", "Х").replace("x", "х").replace("J", "Ж")
                    .replace("j", "ж").replace("’", "ъ");
            }
            return latvalue.replace("обект", "объект").replace("еълон", "эълон");
        }
        function update_lat(updatelat) {
            return updatelat.slice(0, -1).replace("O'", "O‘").replace("G'", "G‘").replace("g'", "g‘").replace("E'", "E’").replace("e'", "e’").replace("a'", "a’")
                .replace("g'", "g‘").replace("o'", "o‘");
        }
    </script>
    <script>
        function goSearchMain() {
           

            var _url = "";

            var val = $('#act_type_0').val();
            if (val != "")
                _url += "&act_type=" + val;

            var from = $('#df_0').val();
            var to = $('#dt_0').val();
            var exdate = $('#checkbox-dp_0').is(':checked') ? "1" : "0";
            if (exdate == "0") {
                if (from != "") {
                    _url += "&from=" + from + "&to=" + from;
                }
            }
            else {
                if (from != "") {
                    _url += "&from=" + from;
                }
                if (to != "") {
                    _url += "&to=" + to;
                }
            }

            val = $('#act_num_0').val().replace(/\s+/g, ' ').trim();
            if (val.length > 30) {
                lxAlert('Matn juda uzun!<br>Hujjat raqami');
                return;
            }
            if (val != "")
                _url += "&actnum=" + val;

            val = $('#act_title_0').val().replace(/\s+/g, ' ').trim();
            if (val.length > 100) {
                lxAlert('Matn juda uzun!<br>Hujjat nomi');
                return;
            }
            if (val != "" && val.length < 3) {
                lxAlert('Matn juda qisqa!<br>Hujjat nomi');
                return;
            }
            if (val != "")
                _url += "&searchtitle=" + val;

            if (_url.length == 0) {
                lxAlert('Hujjat izlash uchun kerakli bo‘lgan shartlar kiritilmagan!');
                return false;
            }

            _url = "/uz/search/all" + "?" + _url.substring(1);
            searchedByEnter = true;
            //myFunctionLoad();
            window.location.href = _url;
        }
    </script>
    <script>
        $(function () {
            var autocompleteShown = false;
            
            $("#act_title_0,#query_1,#query_2,#query_3,#query_4,#query_5,#query_61,#query_62,#query_63,#query_64,#query_65").on('autocomplete.dd.shown', function (evt) {
                autocompleteShown = true;
            });
            
            var searchedByEnter = false;
            $('#act_num_0,#act_title_0').keypress(function (e) {
                if (e.keyCode == 13) {
                    if (autocompleteShown) {
                        autocompleteShown = false;
                        return;
                    }
                    if (!searchedByEnter)
                        goSearchMain();
                }
            });
            $('#actnum_1,#query_1,#query_61').keypress(function (e) {
                if (e.keyCode == 13) {
                    console.log(autocompleteShown);
                    if (autocompleteShown) {
                        autocompleteShown = false;
                        return;
                    }
                    if (!searchedByEnter)
                        goSearch(1);
                }
            });
            $('#actnum_2,#query_2,#query_62').keypress(function (e) {
                if (e.keyCode == 13) {
                    if (autocompleteShown) {
                        autocompleteShown = false;
                        return;
                    }
                    if (!searchedByEnter)
                        goSearch(2);
                }
            });
            $('#actnum_3,#query_3,#query_63').keypress(function (e) {
                if (e.keyCode == 13) {
                    if (autocompleteShown) {
                        autocompleteShown = false;
                        return;
                    }
                    if (!searchedByEnter)
                        goSearch(3);
                }
            });
            $('#actnum_4,#query_4,#query_64').keypress(function (e) {
                if (e.keyCode == 13) {
                    if (autocompleteShown) {
                        autocompleteShown = false;
                        return;
                    }
                    if (!searchedByEnter)
                        goSearch(4);
                }
            });
            $('#actnum_5,#query_5,#query_65').keypress(function (e) {
                if (e.keyCode == 13) {
                    if (autocompleteShown) {
                        autocompleteShown = false;
                        return;
                    }
                    if (!searchedByEnter)
                        goSearch(5);
                }
            });
        });
    </script>
    <script>
        function open_classifiers2() {
            $('#classifiers2_link').hide();
            $('#classifiers2').show();
        }

        
        $(function () {
            $(".lxAutoComplete").autoComplete({
                minLength: 2,
                preventEnter: true,
                noResultsText: '',
                requestThrottling: 300
            });

