    <script>
        $(document).ready(function () {
            $('[data-toggle="tooltip"]').tooltip();
            if (sessionStorage.getItem("img01") != "haydov") {
                $('#myModal1').modal('show');
                sessionStorage.setItem("img01", "haydov");
            }
        });

        if (localStorage.getItem("speak") === "audio") {
            window.onload = function () {
                document.getElementById("startRecognizeOnceAsyncButton").click();
            }
            localStorage.removeItem("speak");
        }

    </script>

    <script type="text/javascript">
        var startRecognizeOnceAsyncButton;
        var subscriptionKey, serviceRegion;
        var SpeechSDK;
        var recognizer;
        var inputtext;
        let resulttext = "";

        document.addEventListener("DOMContentLoaded", function () {
            startRecognizeOnceAsyncButton = document.getElementById("startRecognizeOnceAsyncButton");
            subscriptionKey = "eb54b215b96946b48519cd34ceedc016"
            serviceRegion = "southeastasia"

            inputtext = document.getElementById("act_title_0");

            $('#startRecognizeOnceAsyncButton').click(function () {
                $('#myModal').modal('show');

                if (subscriptionKey === "" || subscriptionKey === "subscription") {
                    alert(
                        "Please enter your Microsoft Cognitive Services Speech subscription key!");
                    return;
                }
                var speechConfig = SpeechSDK.SpeechConfig.fromSubscription(subscriptionKey,
                    serviceRegion);

                speechConfig.speechRecognitionLanguage = "uz-UZ";

                var audioConfig = SpeechSDK.AudioConfig.fromDefaultMicrophoneInput();

                recognizer = new SpeechSDK.SpeechRecognizer(speechConfig, audioConfig);

                recognizer.recognizeOnceAsync(
                    function (result) {
                        if ("oz" == "uz") {
                            resulttext = update_lat(result.text);
                            inputtext.value = latKirl(resulttext);
                            $('#myModal').modal('hide');
                            goSearchMain();
                            recognizer.close();
                            recognizer = undefined;
                        }
                        else {
                            inputtext.value = update_lat(result.text);
                            $('#myModal').modal('hide');
                            goSearchMain();
                            recognizer.close();
                            recognizer = undefined;
                        }

                    },
                    function (err) {
                        alert("Sizning qurilmangizda ovoz uzatish bilan bog'liq muammo mavjud. Iltimos, mikrofon qurilmasi mavjudligini tekshirib, qayta urinib ko‘ring!");
                        recognizer.close();
                        recognizer = undefined;
                    }

                );

                let i = 8;
                let interval = setInterval(() => {
                    timenow()
                }, 1000);

                function timenow() {
                    i = i - 1;
                    if (recognizer === undefined || i === 0) {
                        startRecognizeOnceAsyncButton.style.color = '#495057';
                        $('#myModal').modal('hide');
                    }
                }
            });
        });
    </script>
