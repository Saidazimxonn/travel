<script>

    const htmlEl = document.getElementsByTagName('body')[0];

    if (localStorage.getItem("themes") === "light") {
        htmlEl.dataset.theme = 'light';
    }
    else if (localStorage.getItem("themes") === "grey") {
        htmlEl.dataset.theme = 'grey';
    }
    else if (localStorage.getItem("themes") === "dark") {
        htmlEl.dataset.theme = 'dark';
    }
    const toggleTheme = (theme) => {
        htmlEl.dataset.theme = theme;
        localStorage.setItem("themes", htmlEl.dataset.theme);
    }
</script>

