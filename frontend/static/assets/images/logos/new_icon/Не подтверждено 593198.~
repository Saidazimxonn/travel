<script>
    function goSearch(num) {
        var _url = "";

        var lang = $('#lang_' + num).val();
        if (lang != "") {
            _url += "&lang=" + lang;
        }

        if (num < 5) {
            var form_id = $('#form_' + num).val();
            if (form_id != "") {
                _url += "&form_id=" + form_id;
            }

            var body_id = $('#organ_' + num).val();
            if (body_id != "") {
                _url += "&body_id=" + body_id;
            }
        }
        if (num == 6) {
            var body_id = $('#organ_' + num).val();
            if (body_id != "") {
                _url += "&body_id=" + body_id;
            }
        }

        var status = $('#status_' + num).val();
        if (status != "") {
            _url += "&status=" + status;
        }

        if (num == 1 || num == 6) {
            var nature = $('#nature_' + num).val();
            if (nature != "") {
                _url += "&nature=" + nature;
            }
        }
        if (num == 3) {
            var choose_country = $('#choose_country').val();
            if (choose_country != "") {
                _url += "&choose_country=" + choose_country;
            }

            var level_id = $('#level_id').val();
            if (level_id != "") {
                _url += "&level_id=" + level_id;
            }

            var countries = $('#countries').val();
            if (countries != "") {
                _url += "&countries=" + countries;
            }
        }

        if (num == 5) {
            var court_form = $('#court_form').val();
            if (court_form != "") {
                _url += "&court_form=" + court_form;
            }

            var instance = $('#instance').val();
            if (instance != "") {
                _url += "&instance=" + instance;
            }

            var court_type = $('#court_type').val();
            if (court_type != "") {
                _url += "&court_type=" + court_type;
            }

            var court_branch = $('#court_branch').val();
            if (court_branch != "") {
                _url += "&court_branch=" + court_branch;
            }
        }

        var from = $('#df_' + num).val();
        var to = $('#dt_' + num).val();
        var exdate = $('#checkbox-dp_' + num).is(':checked') ? "1" : "0";
        if (exdate == "0") {
            if (from != "") {
                _url += "&from=" + from + "&to=" + from;
            }
        }
        else {
            if (from != "") {
                _url += "&from=" + from;
            }
            if (to != "") {
                _url += "&to=" + to;
            }
        }

        var actnum = $('#actnum_' + num).val().replace(/\s+/g, ' ').trim();
        if (actnum.length > 30) {
            lxAlert('Matn juda uzun!<br>Hujjat raqami');
            return;
        }
        if (actnum != "") {
            _url += "&actnum=" + actnum;
            if (num == 1 && $('#regnummj_1').is(':checked')) {
                _url += "&regnummj=1";
            }
        }

        var searchtitle = $('#query_6' + num).val().replace(/\s+/g, ' ').trim();
        if (searchtitle.length > 100) {
            lxAlert('Matn juda uzun!<br>Hujjat nomi');
            return;
        }
        if (searchtitle != "" && searchtitle.length < 3) {
            lxAlert('Matn juda qisqa!<br>Hujjat nomi');
            return;
        }
        if (searchtitle != "") {
            _url += "&searchtitle=" + searchtitle;
            if ($('#exact_2' + num).is(':checked')) {
                _url += "&exact2=1";
            }
        }

        var query = $('#query_' + num).val().replace(/\s+/g, ' ').trim();
        if (query.length > 100) {
            lxAlert('Matn juda uzun!<br>Matndagi  so‘z yoki jumla');
            return;
        }
        if (query != "" && query.length < 3) {
            lxAlert('Matn juda qisqa!<br>Matndagi  so‘z yoki jumla');
            return;
        }
        if (query != "") {
            query = encodeURIComponent(query);
            _url += "&query=" + query;
            if ($('#exact_' + num).is(':checked')) {
                _url += "&exact=1";
            }
        }

        if (_url.length == 0) {
            lxAlert('Hujjat izlash uchun kerakli bo‘lgan shartlar kiritilmagan!');
            return;
        }

        var search_type = "";

        if (num == 1) {
            search_type = "/uz/search/nat"
        }
        else if (num == 2) {
            search_type = "/uz/search/loc"
        }
        else if (num == 3) {
            search_type = "/uz/search/int"
        }
        else if (num == 4) {
            search_type = "/uz/search/tech"
        }
        else if (num == 5) {
            search_type = "/uz/search/court"
        }
        else if (num == 6) {
            search_type = "/uz/search/oliy"
        }

        if (_url.includes("lang=null")) {
           
            _url = _url.replace("lang=null", "");
        }
      

        _url = search_type + "?" + _url.substring(1);

        //myFunctionLoad();
        //alert(_url);

        window.location.href = _url;
    }

    function clearSearchForm(num) {
        $('#actnum_' + num).val('');
        if (num == 1 || num == 6) {
            $('#regnummj_1').prop('checked', false);
            $('#nature_' + num).val('').trigger('change');
        }

        $('#query_' + num).val('');
        $('#exact_' + num).prop('checked', false);

        $('#query_6' + num).val('');
        $('#exact_2' + num).prop('checked', false);

        if (num < 7) {
            $('#form_' + num).val('').trigger('change');
            $('#organ_' + num).val('').trigger('change');
        }

        $('#df_' + num).val('');
        $('#dt_' + num).val('');
        if ($('#checkbox-dp_' + num).is(':checked'))
            $('#checkbox-dp_' + num).click();

        $('#lang_' + num).val('').trigger('change');
        $('#status_' + num).val('').trigger('change');

        if (num == 3) {
            $('#choose_country').val('').trigger('change');
            $('#level_id').val('').trigger('change');
            $('#countries').val('').trigger('change');
        }

        if (num == 5) {
            $('#court_form').val('').trigger('change');
            $('#instance').val('').trigger('change');
            $('#court_type').val('').trigger('change');
            $('#court_branch').val('').trigger('change');
        }
    }
</script>
